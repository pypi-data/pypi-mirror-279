<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="crm.stage" id="stage_lead_donation_new">
            <field name="name">New</field>
            <field name="sequence">1</field>
            <field name="team_id" ref="crm_team_sales_donation"/>
        </record>
        <record model="crm.stage" id="stage_lead_donation_in_progress">
            <field name="name">In progress</field>
            <field name="sequence">2</field>
            <field name="team_id" ref="crm_team_sales_donation"/>
        </record>
        <record model="crm.stage" id="stage_lead_donation_completed">
            <field name="name">Completed</field>
            <field name="sequence">3</field>
            <field name="is_won">True</field>
            <field name="probability">100.0</field>
            <field name="on_change">True</field>
            <field name="team_id" ref="crm_team_sales_donation"/>
        </record>
        <record model="crm.stage" id="stage_lead_donation_lost">
            <field name="name">Lost</field>
            <field name="sequence">4</field>
            <field name="team_id" ref="crm_team_sales_donation"/>
        </record>
    </data>

</odoo>