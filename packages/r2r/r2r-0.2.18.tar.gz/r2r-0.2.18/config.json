{
  "app": {
    "max_logs_per_request": 100,
    "max_file_size_in_mb": 32
  },
  "completions": {
    "provider": "litellm"
  },
  "embedding": {
    "provider": "openai",
    "base_model": "text-embedding-3-small",
    "base_dimension": 512,
    "batch_size": 128,
    "text_splitter": {
      "type": "recursive_character",
      "chunk_size": 512,
      "chunk_overlap": 20
    },
    "rerank_model": "None"
  },
  "kg": {
    "provider": "None",
    "batch_size": 1,
    "text_splitter": {
      "type": "recursive_character",
      "chunk_size": 2048,
      "chunk_overlap": 0
    }
  },
  "eval": {
    "provider": "local",
    "llm":{
      "model": "gpt-4o",
      "provider": "openai"
    }
  },
  "ingestion":{
    "selected_parsers": {
      "csv": "default",
      "docx": "default",
      "html": "default",
      "json": "default",
      "md": "default",
      "pdf": "default",
      "pptx": "default",
      "txt": "default",
      "xlsx": "default",
      "gif": "default",
      "png": "default",
      "jpg": "default",
      "jpeg": "default",
      "svg": "default",
      "mp3": "default"
    }
  },
  "logging": {
    "provider": "local",
    "log_table": "logs",
    "log_info_table": "log_info"
  },
  "prompt": {
    "provider": "local"
  },
  "vector_database": {
    "provider": "local",
    "collection_name": "demo_vecs"
  }
}
