      "rerank_dimension": 384,
      "rerank_transformer_type": "CrossEncoder",
      "batch_size": 32,
      "text_splitter": {
        "type": "recursive_character",
        "chunk_size": 512,
        "chunk_overlap": 20
      }
    }
